set nu 
colorscheme torte
set clipboard=unnamedplus
     

map <C-n> :SlimuxREPLSendLine<CR>
vmap <C-n> :SlimuxREPLSendSelection<CR>

" show existing tab with 4 spaces width
set tabstop=4 
" when indenting with '>', use 4 spaces width
set shiftwidth=4
" On pressing tab, insert 4 spaces
set expandtab

















